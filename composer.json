{
    "name": "ali/laravel-tickets",
    "description": "Laravel Support Ticketing Package",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelTickets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTickets\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ali Sharafi",
            "email": "a.sharafi1993@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "7.24.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTickets\\ServiceProvider"
            ]
        }
    }
}
